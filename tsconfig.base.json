  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@jetgold/core": ["./packages/core/src"],
      "@jetgold/home-module": ["./packages/home-module/src"]
    }
  },
  "include": [
